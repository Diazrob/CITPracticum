@using CITPracticum.Data.Enum;
@using CITPracticum.ViewModels;
@model Placement
<style>
    /* Custom styles for the navigation tabs */
    .nav-tabs .nav-item.show .nav-link, .nav-tabs .nav-link.active {
        background-color: #ffffff; /* Active tab background color */
        color: #3431f4; /* Active tab text color */
        border-color: #dee2e6 #dee2e6 #fff; /* Borders around the active tab */
        border-radius: 0.25rem; /* Rounded corners on the top */
    }

    .nav-link {
        color: #007bff; /* Non-active tab text color */
        margin-right: 2px; /* Spacing between tabs */
        border: 1px solid transparent; /* Transparent border for non-active tabs */
        border-radius: 0.25rem; /* Rounded corners on the top */
    }

    .nav-tabs {
        border-bottom: 1px solid #dee2e6; /* Bottom border for the whole nav */
    }

    .modal-header, .modal-footer {
        border-bottom: 1px solid #dee2e6;
        border-top: 1px solid #dee2e6;
    }

    .form-label {
        font-weight: bold;
    }

    .modal-logo {
        height: 50px; /* You can adjust the size as needed */
    }

    .modal-body {
        background-color: #f7f7f7;
        padding: 2rem;
    }

    .modal-footer {
        padding: 1rem 2rem;
        background-color: #f7f7f7;
    }

    .submit-btn {
        background-color: #3431f4;
        border-color: #3431f4;
    }

</style>

    <!-- Tab navigation with button -->
    <ul class="nav nav-tabs mb-3" role="tablist">
        <li class="nav-item">
            <a class="nav-link active" href="/PracticumForm/Index">Practicum Forms</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" href="/PracticumForm/StudentSubmittedForms">Student Submitted Forms</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" href="/PracticumForm/EmployerSubmittedForms">Employer Submitted Forms</a>
        </li>
        <li class="nav-item">
        </li>
    </ul>

    @if (User.IsInRole("admin") || User.IsInRole("student"))
    {
        <div class="card" style="cursor: pointer;" data-bs-toggle="modal" data-bs-target="#formFOIPModal">
            <div class="card-body">
                Form FOIP - Student Release
            </div>
        </div>

        <div class="card mt-3" style="cursor: pointer;" data-bs-toggle="modal" data-bs-target="#formIDModal">
            <div class="card-body">
                Form ID - Student Information
            </div>
        </div>
    }

    <!-- List of Forms as Cards -->
    <div class="card mt-3" style="cursor: pointer;" data-bs-toggle="modal" data-bs-target="#formAModal">
        <div class="card-body">
            Form A - Host Information
        </div>
    </div>

    <div class="card mt-3" style="cursor: pointer;" data-bs-toggle="modal" data-bs-target="#formBModal">
        <div class="card-body">
            Form B - Practicum Agreement
        </div>
    </div>

    <div class="card mt-3" style="cursor: pointer;" data-bs-toggle="modal" data-bs-target="#formCModal">
        <div class="card-body">
            Form C - Practicum Evaluation (100 hours)
        </div>
    </div>

    <div class="card mt-3" style="cursor: pointer;" data-bs-toggle="modal" data-bs-target="#formDModal">
        <div class="card-body">
            Form D - Practicum Evaluation (200 hours)
        </div>
    </div>

    @if (User.IsInRole("admin")) // Add an OR operator for when admin submits the exit interview the student can see it on their dashboard but have it readonly.
    {
        <div class="card mt-3" style="cursor: pointer;" data-bs-toggle="modal" data-bs-target="#formExitModal">
            <div class="card-body">
                Exit Interview
            </div>
        </div>
    }


<!-- Form A Modal -->
<div class="modal fade" id="formAModal" tabindex="-1" aria-labelledby="formAModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-fullscreen">
        <div class="modal-content">
            <div class="modal-header justify-content-center" style="background: linear-gradient(to right, #FFFFFF, #FFFFFF, #69a620);">
                <img src="~/images/lc.png" alt="Logo" class="float-start" style="height: 50px;"> <!-- Adjust the height as needed -->
                <h5 class="modal-title text-center w-100" id="formAModalLabel">Host information (Form A)</h5>
                <button type="button" class="btn-close float-end" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body d-flex justify-content-center">
                <div class="mx-auto" style="width: 50%;">
                    <form method="post" asp-action="FormASubmit" enctype="multipart/form-data">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <!-- Student Last Name, First Name, and Student ID -->
                        @if(User.IsInRole("student")) {
                        <div class="row mb-3">

                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.StuLastName" class="form-label">Student Last Name:</label>
                                <input asp-for="PracticumForms.FormA.StuLastName" type="text" class="form-control" value="@Model.Student.LastName">
                                <span asp-validation-for="PracticumForms.FormA.StuLastName" class="text-danger"></span>

                            </div>
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.StuFirstName" class="form-label">Student First Name:</label>
                                <input asp-for="PracticumForms.FormA.StuFirstName" type="text" class="form-control" value="@Model.Student.FirstName">
                                <span asp-validation-for="PracticumForms.FormA.StuFirstName" class="text-danger"></span>
                            </div>
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.StuId" class="form-label">Student ID:</label>
                                <input asp-for="PracticumForms.FormA.StuId" type="text" class="form-control" value="@Model.Student.StuId">
                                <span asp-validation-for="PracticumForms.FormA.StuId" class="text-danger"></span>
                            </div>
                       
                        </div>

                        } else {
                            <div class="row mb-3">
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.StuLastName" class="form-label">Student Last Name:</label>
                                <input asp-for="PracticumForms.FormA.StuLastName" type="text" class="form-control">
                                <span asp-validation-for="PracticumForms.FormA.StuLastName" class="text-danger"></span>
                            </div>
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.StuFirstName" class="form-label">Student First Name:</label>
                                <input asp-for="PracticumForms.FormA.StuFirstName" type="text" class="form-control">
                                <span asp-validation-for="PracticumForms.FormA.StuFirstName" class="text-danger"></span>
                            </div>
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.StuId" class="form-label">Student ID:</label>
                                <input asp-for="PracticumForms.FormA.StuId" type="text" class="form-control">
                                <span asp-validation-for="PracticumForms.FormA.StuId" class="text-danger"></span>
                            </div>
                        </div>
                        }

                        <!-- Program, Host Company, Type of Organization -->
                        <div class="row mb-3">
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.Program" class="form-label">Program: </label>
                                <input asp-for="PracticumForms.FormA.Program" type="text" class="form-control" value="Computer Information Technology" readonly>
                                <span asp-validation-for="PracticumForms.FormA.Program" class="text-danger"></span>
                            </div>
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.HostCompany" class="form-label">Host Company:</label>
                                <input asp-for="PracticumForms.FormA.HostCompany" type="text" class="form-control">
                                <span asp-validation-for="PracticumForms.FormA.HostCompany" class="text-danger"></span>
                            </div>
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.OrgType" class="form-label">Type of organization:</label>
                                <input asp-for="PracticumForms.FormA.OrgType" type="text" class="form-control">
                                <span asp-validation-for="PracticumForms.FormA.OrgType" class="text-danger"></span>
                            </div>
                        </div>

                        <!-- Supervisor's Name, Position, and Email -->
                        <div class="row mb-3">
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.SVName" class="form-label">Supervisor's name:</label>
                                <input asp-for="PracticumForms.FormA.SVName" type="text" class="form-control">
                                <span asp-validation-for="PracticumForms.FormA.SVName" class="text-danger"></span>
                            </div>
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.SVPosition" class="form-label">Supervisor's position:</label>
                                <input asp-for="PracticumForms.FormA.SVPosition" type="text" class="form-control">
                                <span asp-validation-for="PracticumForms.FormA.SVPosition" class="text-danger"></span>
                            </div>
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.SVEmail" class="form-label">Supervisor's e-mail:</label>
                                <input asp-for="PracticumForms.FormA.SVEmail" type="text" class="form-control">
                                <span asp-validation-for="PracticumForms.FormA.SVEmail" class="text-danger"></span>
                            </div>
                        </div>
                        <!-- ... other fields ... -->
                        <div class="row mb-3">
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.SVPhoneNumber" class="form-label">Phone number:</label>
                                <input asp-for="PracticumForms.FormA.SVPhoneNumber" type="text" class="form-control">
                                <span asp-validation-for="PracticumForms.FormA.SVPhoneNumber" class="text-danger"></span>
                            </div>
                        </div>
                        <!-- ... other fields ... -->
                                               
                        <div class="form-group mb-3">
                            <label asp-for="PracticumForms.FormA.SVCredentials" class="form-label">Supervisor's Credentials</label>
                            <div class="col-md-9">
                                <input class="form-check-input" type="checkbox" name="CredentialsList" value="CIT Diploma" /><span class="mx-2">CIT Diploma</span>
                                <input class="form-check-input" type="checkbox" name="CredentialsList" value="IS Degree" /><span class="mx-2">IS Degree</span>
                                <input class="form-check-input" type="checkbox" name="CredentialsList" value="CS Degree" /><span class="mx-2">CS Degree</span>
                            </div>
                            <span asp-validation-for="PracticumForms.FormA.SVCredentials" class="text-danger"></span>
                        </div>
                        <div class="row mb-3">
                            <div>
                                <label asp-for="PracticumForms.FormA.SVCredOther" class="form-label">Other Credentials:</label>
                                <input asp-for="PracticumForms.FormA.SVCredOther" type="text" class="form-control">
                                <span asp-validation-for="PracticumForms.FormA.SVCredOther" class="text-danger"></span>
                            </div>
                        </div>

                        <!-- ... other fields ... -->
                        <div class="row mb-3">
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.Address.Street" class="form-label">Street:</label>
                                <input asp-for="PracticumForms.FormA.Address.Street" type="text" class="form-control">
                            </div>
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.Address.City" class="form-label">City:</label>
                                <input asp-for="PracticumForms.FormA.Address.City" type="text" class="form-control">
                            </div>
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.Address.Prov" class="form-label">Province:</label>
                                <input asp-for="PracticumForms.FormA.Address.Prov" type="text" class="form-control">
                            </div>
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.Address.Country" class="form-label">Country:</label>
                                <input asp-for="PracticumForms.FormA.Address.Country" type="text" class="form-control">
                            </div>
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.Address.PostalCode" class="form-label">Postal Code:</label>
                                <input asp-for="PracticumForms.FormA.Address.PostalCode" type="text" class="form-control">
                            </div>
                        </div>
                        <!-- ... other fields ... -->
                        <div class="row mb-3">
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormA.StartDate" class="form-label">Date you expect to begin:</label>
                                <input asp-for="PracticumForms.FormA.StartDate" type="date" class="form-control">
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col-md-4">
                                <div class="form-check form-check-inline px-0">
                                    <label asp-for="PracticumForms.FormA.PaymentCategory" class="form-label">Paid or Unpaid:</label>
                                    <select asp-for="PracticumForms.FormA.PaymentCategory" asp-items="@Html.GetEnumSelectList<PaymentCategory>()" class="form-control">
                                        <option>--Select--</option>
                                    </select>
                                    <span asp-validation-for="PracticumForms.FormA.PaymentCategory" class="text-danger"></span>
                                </div>
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col-md-5">
                                <div class="form-check form-check-inline px-0">
                                    <label for="workOutsideCountry" class="form-label">Will you be working outside of the country?</label>
                                    <select id="workOutsideCountry" name="workOutsideCountry" class="form-control">
                                        <option value="">--Select--</option>
                                        <option value="Yes">Yes</option>
                                        <option value="No">No</option>
                                    </select>
                                    <span class="text-danger"></span>
                                </div>
                            </div>
                        </div>
                        <div class="mb-3 text-center">
                            <p>Aside from the forms available on this webpage, the Program Assistant will look after all documents required for your host. Formal practicum agreements and evaluation forms will be supplied to your host.</p>
                        </div>
                        <div class="modal-footer justify-content-center">
                            <input type="button" class="btn btn-danger" value="Cancel" />
                            <input type="submit" class="btn btn-success" value="Submit" />
                            <input type="button" id="openFormB" class="btn btn-primary" value="Next" />
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Form B Modal -->
<div class="modal fade" id="formBModal" tabindex="-1" aria-labelledby="formBModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-fullscreen">
        <div class="modal-content">
            <div class="modal-header justify-content-center" style="background: linear-gradient(to right, #FFFFFF, #FFFFFF, #69a620);">
                <img src="~/images/lc.png" alt="Logo" class="float-start" style="height: 50px;">
                <h5 class="modal-title text-center w-100" id="formFOIPModalLabel">Practicum Agreement between Host and Student (Form B)</h5>
                <button type="button" class="btn-close float-end" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body d-flex justify-content-center">
                <div class="mx-auto" style="width: 50%;">
                    <form>
                        <div class="row mb-3">
                            <div class="col-md-3">
                                <label for="pracHost" class="form-label">Practicum Host:</label>
                                <input type="text" name="pracHost" class="form-control" />
                            </div>
                            <div class="col-md-9" style="margin-top: 2.5rem !important;">
                                agrees to provide a meaningful work experience for
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col-md-3">
                                <label for="studName" class="form-label">Student's Name:</label>
                                <input type="text" name="studName" class="form-control" />
                            </div>
                            <div class="col-md-1" style="margin-top: 2.5rem !important;">
                                from
                            </div>
                            <div class="col-md-3">
                                <label for="studName" class="form-label">Start Date:</label>
                                <input type="date" class="form-control">
                            </div>
                            <div class="col-md-1" style="margin-top: 2.5rem !important;">
                                to
                            </div>
                            <div class="col-md-3">
                                <label for="studName" class="form-label">End Date:</label>
                                <input type="date" class="form-control">
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col-md-12">
                                based on the student’s skills gained in the Lethbridge College <b>Computer Information Technology</b>
                                program. The Practicum Host is responsible for the student’s final evaluation, using the Student
                                Evaluation form included in this package and in the host information package.
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col-md-12">
                                The student shall:
                                <ul>
                                    <li>
                                        Maintain a professional attitude and a high level of participation.
                                    </li>
                                    <li>
                                        Treat this practicum as a job and should adhere to all company employee policies.
                                    </li>
                                    <li>
                                        Track hours worked and report them to the Program Assistant through <b>Form D</b>.
                                    </li>
                                    <li>
                                        Maintain contact with the lead instructor during their practicum.
                                    </li>
                                    <li>
                                        Respect schedules and meet deadlines.
                                    </li>
                                    <li>
                                        Conduct themselves appropriately.
                                    </li>
                                    <li>
                                        Acknowledges that the Host and/or College has the right to terminate an unsatisfactory placement.
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col-md-4">
                                <label for="formBOrgName" class="form-label">Organization Name:</label>
                                <input type="text" name="formBOrgName" class="form-control" />
                            </div>
                            <div class="col-md-4">
                                <label for="formBPrac" class="form-label">Practicum Supervisor:</label>
                                <input type="text" name="formBPrac" class="form-control" />
                            </div>
                            <div class="col-md-4">
                                <label for="formBAddress" class="form-label">Address:</label>
                                <input type="text" name="formBAddress" class="form-control" />
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col-md-4">
                                <label for="formBPosition" class="form-label">Position:</label>
                                <input type="text" name="formBPosition" class="form-control" />
                            </div>
                            <div class="col-md-4">
                                <label for="formBEmail" class="form-label">Email Address:</label>
                                <input type="text" name="formBEmail" class="form-control" />
                            </div>
                            <div class="col-md-4">
                                <label for="formBPhone" class="form-label">Phone Number:</label>
                                <input type="text" name="formBPhone" class="form-control" />
                            </div>
                        </div>
                        <!-- Content for Form B -->
                        <div class="modal-footer sticky justify-content-center">
                            <input type="button" class="btn btn-danger" value="Cancel" />
                            <input type="submit" class="btn btn-success" value="Submit" />
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Form C Modal -->
<div class="modal fade" id="formCModal" tabindex="-1" aria-labelledby="formCModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-fullscreen">
        <div class="modal-content">
            <div class="modal-header justify-content-center" style="background: linear-gradient(to right, #FFFFFF, #FFFFFF, #69a620);">
                <img src="~/images/lc.png" alt="Logo" class="float-start" style="height: 50px;">
                <h5 class="modal-title text-center w-100" id="formFOIPModalLabel">CIT 2271 – IT Practicum Evaluation (Form C - 100 hours)</h5>
                <button type="button" class="btn-close float-end" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body d-flex justify-content-center">
                <div class="mx-auto" style="width:50%;">
                    <form method="post" asp-action="FormCSubmit" enctype="multipart/form-data">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <div class="row mb-3">
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormC.PracSV" class="form-label">Practicum Supervisor:</label>
                                <input asp-for="PracticumForms.FormC.PracSV" type="text" class="form-control">
                            </div>
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormC.Org" class="form-label">Organization:</label>
                                <input asp-for="PracticumForms.FormC.Org" type="text" class="form-control">
                            </div>
                            <div class="col-md-4">
                                <label asp-for="PracticumForms.FormC.StuName" class="form-label">Student Name:</label>
                                <input asp-for="PracticumForms.FormC.StuName" type="text" class="form-control" value="@Model.Student.FirstName">
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col-md-2">
                                <p>Faculty Advisor:</p>
                            </div>
                            <div class="col-md-8">
                                <ul style="list-style-type: none; margin-left: 0; padding-left: 0;">
                                    <li><h6 class="mb-2">Stephen Graham</h6></li>
                                    <li><p class="mb-2"><a href="mailto:stephen.graham@lethbridgecollege.ca">stephen.graham@lethbridgecollege.ca</a></p></li>
                                    <li><p><a href="tel:4033203202">(403) 320-3202 ext 5794</a></p></li>
                                </ul>
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col-md-2">
                                <p>Program Assistant:</p>
                            </div>
                            <div class="col-md-8">
                                <ul style="list-style-type: none; margin-left: 0; padding-left: 0;">
                                    <li><h6 class="mb-2">Toni Gfrerer</h6></li>
                                    <li><p><a href="mailto:toni.gfrerer@lethbridgecollege.ca">toni.gfrerer@lethbridgecollege.ca</a></p></li>
                                </ul>
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col-md-12">
                                <ol>
                                    <li>Please complete this form after the student has finished 100 hours of work</li>
                                    <li>Discuss the evaluation with the student and provide them a copy.</li>
                                    <li>Return the completed copy to the Faculty Advisor or Program Assistant.</li>
                                </ol>
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col-md-12">
                                <p><b>Please use the following ratings in evaluating the practicum students.</b></p>
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col-md-2">
                                <p>Excellent:</p>
                            </div>
                            <div class="col-md-10">
                                <p>Meets professional level standards</p>
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col-md-2">
                                <p>Good:</p>
                            </div>
                            <div class="col-md-10">
                                <p>Adequately fulfills expectations</p>
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col-md-2">
                                <p>Fair:</p>
                            </div>
                            <div class="col-md-10">
                                <p>
                                    Inconsistent, needs work to achieve acceptable standards.
                                    Please explain “fair” ratings in “comments” section.
                                </p>
                            </div>
                        </div>
                        <table class="table">
                            <thead>
                                <tr>
                                    <th scope="col">A.</th>
                                    <th scope="col">Professionalism</th>
                                    <th scope="col"><center>Rating</center></th>

                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>1.</td>
                                    <td>The student is on time, organized and prepared to work.</td>
                                    <td>

                                        <div class="d-flex align-items-center">
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" asp-for="PracticumForms.FormC.A1" value="Excellent" checked>
