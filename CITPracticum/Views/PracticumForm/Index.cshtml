                                <span asp-validation-for="FormA.StuFirstName" class="text-danger"></span>
                            </div>
                            <div class="col-md-4">
                                <label asp-for="FormA.StuId" class="form-label">Student ID:</label>
                                <input asp-for="FormA.StuId" type="text" class="form-control">
                                <span asp-validation-for="FormA.StuId" class="text-danger"></span>
