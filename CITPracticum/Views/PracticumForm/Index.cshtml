                            <div class="col-md-4">
                                <label asp-for="FormA.OrgType" class="form-label">Type of organization:</label>
                                <input asp-for="FormA.OrgType" type="text" class="form-control">
                                <span asp-validation-for="FormA.OrgType" class="text-danger"></span>
                            </div>
